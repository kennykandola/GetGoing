<br>
<div class='w-container'>
  <div class='table-responsive' id='users_table'>
    <table class='table table-striped table-bordered table-hover'
      <thead>
        <tr>
          <th> Actor </th>
          <th> Notification </th>
        </tr>
      </thead>
      <tbody>
        <% @notifications.each do |notification| %>
          <tr>
            <td>
              <%=link_to image_tag(notification.actor.picture_url, height: "35px", class: 'actor-image'), "#" %>
            </td>
            <td>
              <%= notifiable_link(notification)%>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
